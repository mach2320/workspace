<style>
    p {
        font-size: 1.2rem;
        font-weight: bold;
    }
</style>

<body>
    <!-- Checkout Section Begin -->
    <section class="checkout spad">
        <div class="container">
            <div class="checkout__form">
                <h1>상품 등록</h1>
                <br><br>
                <form action="insertProduct/insert" id="insertProduct" method="post" enctype="multipart/form-data"
                    autocomplete="off" accept-charset="UTF-8">
                    <div class="row">
                        <div class="col-lg-12 col-md-12">
                            <div class="checkout__input">
                                <p>물품명<span>*</span></p>
                                <input type="text" name="productName" required>
                            </div>
                            <div class="checkout__input">
                                <p>물품설명<span>*</span></p>
                                <textarea id="productDetail" name="productDetail" required></textarea>
                            </div>
                            <div class="checkout__input">
                                <p>물품가격<span>*</span></p>
                                <input type="number" name="productPrice" required>
                            </div>
                            <div class="checkout__input">
                                <p>물품분류<span>*</span></p>
                                <input type="text" name="productDiv" required>
                            </div>
                            <div class="checkout__input">
                                <p>물품수량<span>*</span></p>
                                <input type="number" name="productCount" required>
                            </div>
                            <div class="checkout__input">
                                <p>대표 이미지<span>*</span></p>
                                <input type="file" name="file" id="file">
                                <p>추가 이미지</p>
                                <input type="file" name="file" id="file">
                                <input type="file" name="file" id="file">
                                <input type="file" name="file" id="file">
                                <input type="file" name="file" id="file">
                                <input type="file" name="file" id="file">
                            </div>
                        </div>
                    </div>
                    <button type="submmit" class="site-btn" id="insert_btn"> 상품 등록</button>
                </form>
            </div>
        </div>
    </section>
</body>